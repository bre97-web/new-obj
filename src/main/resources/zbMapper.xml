<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace=绑定一个对应的mapper接口-->
<mapper namespace="com.mapper.accountBookMapper">
    <!--    查询语句-->
    <select id="getall" resultType="com.bean.accountBook">
        select * from accountbook
    </select>
    <select id="getUserAll" resultType="com.bean.accountBook">
        select * from accountbook where u_id=#{u_id}
    </select>
    <insert id="pushOne">
        insert into accountbook(u_id, szqk, money, lx, bz) values (#{u_id},#{szqk},#{money},#{lx},#{bz})
    </insert>

</mapper>